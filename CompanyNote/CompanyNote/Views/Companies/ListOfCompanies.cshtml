@model IEnumerable<CompanyNote.Models.Company>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CompanyName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DropDownListFor(modelItem => item.CompanyName, "")
        </td>
    </tr>
}

</table>
